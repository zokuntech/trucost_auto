# API Settings
API_V1_STR=/api/v1
PROJECT_NAME=TruCost Auto

# Security
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS
BACKEND_CORS_ORIGINS=["*"]

# File Upload
MAX_UPLOAD_SIZE=10485760  # 10MB in bytes
ALLOWED_FILE_TYPES=["pdf", "jpg", "jpeg", "png", "txt"]

# OpenAI API Key (Required for Quote Parser Agent & Research LLM Extraction)
OPENAI_API_KEY=your_openai_api_key_here
# OPENAI_MODEL=gpt-4o-mini

# # Firecrawl API Key (No longer used)
# FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Search API Key (Google Custom Search API)
SEARCH_API_KEY=your_google_api_key_here
# Google Custom Search Engine ID (CX)
GOOGLE_CSE_ID=your_google_cse_id_here

# Celery Configuration (using Redis)
# Use different database numbers (e.g., /0 and /1) for broker and backend
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/1 